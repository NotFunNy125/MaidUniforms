    <apparel>
        <bodyPartGroups>
            <li>Torso</li>
            <li>Shoulders</li>
        </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Maid/Cleaner/CleanerMaid</wornGraphicPath>
            <useWornGraphicMask>true</useWornGraphicMask>
        <layers>
            <li>Shell</li>
        </layers>
        <tags>
            <li>IndustrialBasic</li>
        </tags>
        <defaultOutfitTags>
            <li>Worker</li>
        </defaultOutfitTags>
        <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
    <tradeTags>
        <li>BasicClothing</li>
    </tradeTags>
    </ThingDef>

