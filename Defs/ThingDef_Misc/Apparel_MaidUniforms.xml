<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_CleanerMaid</defName>
    <label>Cleaner Maid Uniform</label>
    <description>A simple Maid Uniform</description>
    <recipeMaker>
        <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
        <texPath>Things/Pawn/Humanlike/Apparel/Maid/Cleaner/CleanerMaid</texPath>
